function [ ] = set_antiparallel( state )
%SET_ANTIPARALLEL Switch the relays to antiparallel (or not) currents.
%   Drives the current to zero first to prevent arcing in the relays, the
%   resets to the original current.

global config;

if config.set_current == [ 0 0 0 ]
   set_redlab_bit('antip
end
    orig=config.set_current;
    set_current([0 0 0]);
    



end

